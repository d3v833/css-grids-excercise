{
  "name": "realestate-template",
  "version": "1.0.0",
  "description": "Portfolio project - Realestate Site",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass sass/main.scss css/style.css",
    "devserver": "live-server",
    "start": "npm-run-all",
    "compile:sass": "node-sass sass/main.scss css/style.comp. -w",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' css/style.comp.css -o css/style.prefix.css",
    "compress:css": "node-sass css/style.prefix.css css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass prefix:css compress:css"
  },
  "author": "developer833",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^7.1.4",
    "concat": "^1.0.3",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.1",
    "postcss-cli": "^6.1.2"
  },
  "dependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.2.0",
    "prettier": "^1.17.1",
    "sass": "^1.20.1",
    "scss": "^0.2.4"
  }
}
